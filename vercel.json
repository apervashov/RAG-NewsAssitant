{
  "version": 2,
  "builds": [
    { "src": "pure-server.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/pure-server.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/pure-server.js" }
  ]
} 