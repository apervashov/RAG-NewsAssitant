{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/stream", "dest": "/api/stream.js" },
    { "src": "/api/agent", "dest": "/api/agent.js" },
    { "src": "/(.*\\.js|.*\\.css|.*\\.png|.*\\.jpg|.*\\.svg|.*\\.ico)", "dest": "/api/static.js" },
    { "src": "/", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
} 